{
    "name": "ticketswap/phpstan-error-formatter",
    "description": "A minimalistic error formatter for PHPStan",
    "keywords": ["phpstan", "dev"],
    "type": "phpstan-extension",
    "require": {
        "php": "^7.4|^8.0",
        "phpstan/phpstan": "^1.12.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TicketSwap\\PHPStanErrorFormatter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TicketSwap\\PHPStanErrorFormatter\\": "tests/"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}
